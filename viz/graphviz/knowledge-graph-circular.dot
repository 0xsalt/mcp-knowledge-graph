    subgraph cluster_0 {
        label="Identity";
        style=rounded;
        bgcolor="red20";
        fontcolor="red";
        
        default_user [
            label="default_user",
            fillcolor="red",
            fontcolor=white,
            tooltip="default_user\nType: Identity\nCategory: Identity"
        ];
    }

